connect$pppoe(r0, &(0x7f0000000080)={0x18, 0x0, {0x4, @remote, 'vlan1\x00'}}, 0x1e)
ioctl$PPPOEIOCSFWD(r1, 0x4008b100, &(0x7f0000000500)={0x18, 0x0, {0x2, @dev={'\xaa\xaa\xaa\xaa\xaa', 0x17}, 'hsr0\x00'}})
